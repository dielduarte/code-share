{
  "name": "code-share",
  "displayName": "code-share",
  "description": "an extension to share your project or especific file from vscode to world",
  "version": "0.0.1",
  "publisher": "code-share",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeshare.shareFile"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "codeshare.shareFile",
        "title": "codeshare.shareFile"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
